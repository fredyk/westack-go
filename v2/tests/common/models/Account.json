{
  "name": "Account",
  "base": "Account",
  "plural": "accounts",
  "public": true,
  "properties": {
    "email": {
      "type": "string",
      "required": true
    }
  },
  "relations": {
    "notes": {
      "type": "hasMany",
      "model": "Note"
    }
  },
  "casbin": {
    "policies": [
      "$everyone,*,westackAuthorize,allow",
      "$everyone,*,westackToken,allow",
      "$everyone,*,westackUserInfo,allow"
    ]
  }
}
